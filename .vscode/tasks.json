{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "dld-remote-debug",
        "type": "shell",
        "command": "dld rdb -p 9013 project-management-go-worker-1 ",
        "isBackground": true,
        "problemMatcher": [
          {
            "pattern": [
              {
                "regexp": ".",
                "file": 1,
                "location": 2,
                "message": 3
              }
            ],
            "background": {
              "activeOnStart": true,
              "beginsPattern": ".",
              "endsPattern": "."
            }
          }
        ]
      }
    ]
  }